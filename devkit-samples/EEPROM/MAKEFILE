#====================================================================
#       Makefile for DEMO CODE for erasing and readin EEPROM
#====================================================================
#
#====================================================================
#       Macro & Assembler flags
#====================================================================

STADDR = 4000
MACFLAGS = -fb
ALNFLAGS = -l -e -a $(STADDR) x x

#====================================================================
#       Default Rules
#====================================================================

.SUFFIXES: .o .s
.s.o:
	mac $(MACFLAGS) $<


#====================================================================
#       EXECUTABLES
#====================================================================

test_e2.cof: test_e2.o eeprom.o
	aln $(ALNFLAGS) test_e2.o eeprom.o

#====================================================================
#       EOF
#====================================================================
